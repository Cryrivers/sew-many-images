{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
        "semi": 2,
        "no-use-before-define": ["error", { "functions": false, "classes": false }],
        "import/prefer-default-export": "off"
    }
}
